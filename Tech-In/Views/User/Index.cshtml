            <!-- ***************Modals********************-->
            @* Personal Model *@
            <div class="modal fade" id="personalModal" tabindex="-1" role="dialog">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            <h4 class="modal-title">Personal Information</h4>
                        </div>

                        <form method="post">
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <!--First Name-->
                                        <div class="form-group">
                                            <label for="recipient-name" class="control-label">First Name:</label>
                                            <input type="text" class="input" asp-for="UserPersonalVM.FirstName">
                                        </div>
                                        <!--/First Name-->
                                    </div>
                                    <div class="col-sm-6">
                                        <!--Last Name-->
                                        <div class="form-group">
                                            <label for="recipient-name" class="control-label">Last Name:</label>
                                            <input type="text" class="input" asp-for="UserPersonalVM.LastName">
                                        </div>
                                        <!--/Last Name-->
                                    </div>
                                </div>
                                <!--Phone No-->
                                <div class="form-group">
                                    <label for="recipient-name" class="control-label">Phone No:</label>
                                    <input type="text" class="input" asp-for="UserPersonalVM.PhoneNo">
                                </div>
                                <!--/Phone No-->
                                <!--Email-->
                                <div class="form-group">
                                    <label for="recipient-name" class="control-label">Email:</label>
                                    <input type="email" class="input" asp-for="UserPersonalVM.Email" disabled>
                                </div>
                                <!--/Email-->
                                <!--DOB-->
                                <div class="form-group">
                                    <label for="recipient-name" class="control-label">Date of Birth:</label>
                                    <input type="date" class="input" asp-for="UserPersonalVM.DOB">
                                </div>
                                <!--/DOB-->
                                <!--Gender-->

                                <div class="form-group">
                                    <div>
                                        <label asp-for="UserPersonalVM.Gender">
                                            @Html.RadioButtonFor(model => model.UserPersonalVM.Gender, new { id = "sex" })
                                            @Html.Label("sex" + Gender.Male, "Male")
                                        </label>

                                        @Html.RadioButtonFor(model => model.UserPersonalVM.Gender, new { id = "sex" })
                                        @Html.Label("sex" + Gender.Female, "Female")

                                    </div>
                                </div>
                                <!--/Gender-->
                                <!--Country-->
                                <div class="form-group">
                                    <label asp-for="UserPersonalVM.Country" class="control-label"></label>
                                    <select class="form-control" asp-for="UserPersonalVM.Country.CountryID" asp-items="Model.UserPersonalVM.Countries"></select>
                                </div>
                                <!--/Country-->
                                <!--City-->
                                <div class="form-group">
                                    <label asp-for="UserPersonalVM.City" class="control-label"></label>
                                    <select class="form-control" asp-for="UserPersonalVM.City.CityID" asp-items="Model.UserPersonalVM.Cities"></select>
                                </div>
                                <!--/City-->
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="white-btn btn-default" data-dismiss="modal">Close</button>
                                <button asp-action="UpdatePersonalDetails" class="main-btn">Save</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            @* /Personal Model *@
            @* Experience Add *@

            <div class="modal fade" id="addExperienceModal" tabindex="-1" role="dialog">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
