                                    <label for="recipient-name" class="control-label">Location:</label>
                                    <input type="text" class="input">
                                </div>
                                <!--/Location-->
                                <!--Text Area-->
                                <div class="form-group">
                                    <label for="recipient-name" class="control-label">Description:</label>
                                    <textarea class="input" placeholder="Description"></textarea>
                                </div>
                                <!--/Text Area-->
                                <div class="row">
                                    <div class="col-sm-6">
                                        <!--Start Date-->
                                        <div class="form-group">
                                            <label for="recipient-name" class="control-label">From:</label>
                                            <input type="date" class="input">
                                        </div>
                                        <!--/Start Date-->
                                    </div>
                                    <div class="col-sm-6">
                                        <!--End Date-->
                                        <div class="form-group">
                                            <label for="recipient-name" class="control-label">To:</label>
                                            <input type="date" class="input" id="recipient-name">
                                        </div>
                                        <!--/End Date-->
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="custom-control custom-checkbox">
                                        <input class="custom-control-input" id="customCheck1" checked="" type="checkbox">
                                        <label for="customCheck1">I currently work here</label>
                                    </div>
                                </div>

                            </form>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="white-btn btn-default" data-dismiss="modal">Close</button>
                            <button type="button" class="main-btn">Save</button>
                        </div>
                    </div>
                </div>
